<!--Section-->
<section id="hero_youtube_intro">

	{# <img class="jarallax-img" src="{{ page.media.images[page.header.backdrop].url }}" alt="" loading="lazy"> #}
	<div class="row no-gutters">
		<div class="col-lg-6 vlt-column-space-to-container d-flex align-items-center to-left">
			<div>
				{# <!--Particle image--><img class="vlt-particle" src="{{ url('theme://images/root/square-plus-pattern.png') }}" data-animation-name="zoomIn" alt="" style="position: absolute; bottom: 50px; right: 0; max-width: 220px;opacity:0.1" loading="lazy"> #}

				<!--Animated block-->
					<div class="vlt-animated-block" data-aos="fade" data-aos-delay="100"> <h3>{{ page.header.smallHeading }}</h3>
				</div>

				<!--Animated block-->
				<div class="vlt-animated-block">
					<div
						style="color: #d9d9d9;">
						<!--Stroke text-->
						<div class="vlt-stroke-text vlt-stroke-text--lg" data-aos="fade-right" data-aos-delay="200">ZSB
						</div>
					</div>
					<h2 class="negative-top-offset" data-aos="fade-left" data-aos-delay="250">{{page.header.heading}}</h2>
				</div>
				<div class="vlt-gap-20"></div>
				<!--Animated block-->
				<div class="vlt-animated-block" data-aos="fade-up" data-aos-delay="500">
					<p class="subtext">{{page.header.subtext}}</p>
				</div>
				<div class="vlt-gap-50"></div>
				<!--Animated block-->
				<div class="vlt-animated-block">
					<div data-aos="fade" data-aos-delay="900">
					<!--Button-->
					{% if page.header.isCustomButton %}
						{{ page.header.buttonSource | raw}}
					{% else %}
					<a class="vlt-btn vlt-btn--primary vlt-btn--md" href="{{page.header.buttonUrl}}" target="{{page.header.buttonTarget}}">{{page.header.buttonText}}</a>
					{% endif %}
					</div>
				</div>
			</div>
		</div>
		<div class="col-lg-6 d-flex align-items-center justify-content-center">
			
			<div class="to-right">
				<img class="vlt-column-space-to-container" src="{{ page.media.images[page.header.backdrop].url }}" alt="" data-aos="fade-left" data-aos-delay="500" alt="" style="width:100%" loading="lazy">
				<!--Video button-->
				{# <div class="vlt-video-button vlt-particle" data-aos="zoom-in" data-aos-delay="1000">
					<a href="{{ page.header.videoUrl }}" data-fancybox data-small-btn="true" rel="nofollow">
						<i class="icon-play"></i>
					</a>
					<h6>Find Out More</h6>
				</div> #}
			</div>
		</div>

	</div>

	<div class="container">
		<div class="row justify-content-between align-items-center">
			{% for item in page.header.clients %}
			<div class="col-auto">
				<div class="vlt-gap-50"></div>
				<!--Animated block-->
				<div class="vlt-animated-block" data-aos="fade" data-aos-delay="800">
					<!--Partner-->
					<div class="vlt-partner vlt-partner--center"><img src="{{ page.media.images[item.image].cropResize(300, 30).url }}" alt="{{item.name}}" loading="lazy" style="height:30px;">
					</div>
				</div>
			</div>
			{% endfor %}
		</div>
	</div>

</section>
