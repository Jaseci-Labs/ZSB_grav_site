<section>
	<div class="container">
		<div class="row justify-content-between align-items-center">
			{% for item in page.header.clients %}
			<div class="col-auto">
				<div class="vlt-gap-50"></div>
				<!--Animated block-->
				<div class="vlt-animated-block" data-aos="fade" data-aos-delay="0">
					<!--Partner-->
					<div class="vlt-partner vlt-partner--center"><a href="#"><img src="{{ page.media.images[item.image].cropResize(300, 30).url }}" alt="" loading="lazy" style="height:30px;"></a>
					</div>
				</div>
			</div>
			{% endfor %}
		</div>
	</div>
</section>
